{
    "$schema": "http://json.schemastore.org/template",
    "author": "PockyBum522 (David S)",
    "classifications": [ "Windows", "Console" ],
    "identity": "PockyBum522.Console.AppTemplate",
    "name": "PockyBum522 Console App Solution",
    "description": "PockyBum522 new console app template with logging",
    "shortName": "pockyconsole",
    "sourceName": "Unique.Id.To.Be.Replaced",
    "tags": {
        "language": "C#",
        "type": "solution"
    },
    "preferNameDirectory": true,
    "sources": [
        {
            "source": "./",
            "target": "./",
            "include": [ "**/*" ],
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                "**/.template.config",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json",
                "**/.git/**",
                "**/*.nuspec",
                "**/.idea/**"
            ],
            "copyOnly":[
                "**/node_modules/**"
            ]
        }
    ]
}